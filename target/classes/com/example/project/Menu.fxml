<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.MenuController">
   <children>
      <Label layoutX="145.0" layoutY="124.0" prefHeight="112.0" prefWidth="509.0" text="Ping Pong" textAlignment="CENTER">
         <font>
            <Font name="Showcard Gothic" size="96.0" />
         </font>
         <textFill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop>
                     <color>
                        <Color red="0.48148149251937866" green="0.9222221970558167" blue="1.0" />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color red="1.0" green="1.0" blue="1.0" />
                     </color>
                  </Stop>
               </stops>
            </RadialGradient>
         </textFill>
      </Label>
      <Button fx:id="startButton1P" layoutX="31.0" layoutY="405.0" mnemonicParsing="false" onAction="#start1P" onMouseClicked="#start1P" text="1 Player">
         <font>
            <Font name="Showcard Gothic" size="40.0" />
         </font>
      </Button>
      <Button fx:id="startButton2P" layoutX="524.0" layoutY="405.0" mnemonicParsing="false" onAction="#start2P" onMouseClicked="#start2P" text="2 Players">
         <font>
            <Font name="Showcard Gothic" size="40.0" />
         </font>
      </Button>
      <Circle fill="WHITE" layoutX="668.0" layoutY="223.0" radius="13.0" stroke="BLACK" strokeType="INSIDE" />
   </children>
</AnchorPane>
